{
  "name": "@triplit/types",
  "packageManager": "yarn@3.4.1",
  "scripts": {
    "build": "tsc && yarn validate:types",
    "build:watch": "tsc -w",
    "validate:types": "node ../../scripts/check-for-local-references-in-declarations.js dist"
  },
  "exports": {
    "./*": {
      "import": "./src/*",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "devDependencies": {
    "typescript": "^5.2.2"
  }
}
